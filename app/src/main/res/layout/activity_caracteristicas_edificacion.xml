                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_modalidad"
                    android:textSize="@dimen/text_size_small" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="11dp"
                    android:text="SISTEMAS Ventanas *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>
                <Spinner
                    android:id="@+id/spn_sistemas_ventana"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_size_small" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="11dp"
                    android:text="OTRO"
                    android:textColor="#00796B"
                    android:textSize="@dimen/text_size_small" />
                <EditText
                    android:id="@+id/edt_otros_ventana"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:drawable/edit_text"
                    android:paddingStart="15dp"
                    android:paddingEnd="15dp"
                    android:hint="ESPECIFICAR"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:textSize="@dimen/text_size_small" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp"
                android:layout_marginTop="11dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MAMPARAS"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="@color/black"
                    android:textStyle="bold"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MARCO Mamparas"
                    android:layout_marginTop="11dp"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>
                <Spinner
                    android:id="@+id/spn_marco_mamparas"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_size_small" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="VIDRIO Mamparas"
                    android:layout_marginTop="11dp"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>
                <Spinner
                    android:id="@+id/spn_vidrio_mamparas"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_size_small" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SISTEMAS Mamparas"
                    android:layout_marginTop="11dp"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>
                <Spinner
                    android:id="@+id/spn_sistemas_mamparas"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_size_small" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="OTRO"
                    android:layout_marginTop="11dp"
                    android:textColor="#00796B"
                    android:textSize="@dimen/text_size_small" />
                <EditText
                    android:id="@+id/edt_otros_mampara"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:drawable/edit_text"
                    android:paddingStart="15dp"
                    android:paddingEnd="15dp"
                    android:hint="ESPECIFICAR"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:textSize="@dimen/text_size_small" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp"
                android:layout_marginTop="11dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="COCINA Y LAVANDERÍA"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="@color/black"
                    android:textStyle="bold"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PISO Cocina *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>

                <MultiAutoCompleteTextView
                    android:id="@+id/multiCompletePisosCocina"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Seleccione estructuras"
                    android:inputType="text" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PAREDES Cocina *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>

                <MultiAutoCompleteTextView
                    android:id="@+id/multiCompleteParedesCocina"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Seleccione estructuras"
                    android:inputType="text" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="MUEBLES DE COCINA Tipo *"
                        android:textSize="@dimen/text_size_small"
                        android:layout_marginTop="11dp"
                        android:textColor="#00796B"/>
                    <Spinner
                        android:id="@+id/spn_mueble_cocina"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="MUEBLES DE COCINA Tipo *"
                        android:textSize="@dimen/text_size_small"
                        android:layout_marginTop="11dp"
                        android:textColor="#00796B"/>
                    <Spinner
                        android:id="@+id/spn_mueble_cocina2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="TABLEROS *"
                        android:layout_marginTop="11dp"
                        android:textSize="@dimen/text_size_small"
                        android:textColor="#00796B"/>
                    <Spinner
                        android:id="@+id/spn_tableros"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="LAVADEROS *"
                        android:layout_marginTop="11dp"
                        android:textSize="@dimen/text_size_small"
                        android:textColor="#00796B"/>
                    <Spinner
                        android:id="@+id/spn_lavaderos"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_size_small" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="OTRO"
                    android:layout_marginTop="11dp"
                    android:textColor="#00796B"
                    android:textSize="@dimen/text_size_small" />
                <EditText
                    android:id="@+id/edt_otros_cocina"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:drawable/edit_text"
                    android:paddingStart="15dp"
                    android:paddingEnd="15dp"
                    android:hint="ESPECIFICAR"
                    android:paddingTop="10dp"
