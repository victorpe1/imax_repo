                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp"
                android:layout_marginTop="11dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ALBAÑERÍA Y REVOQUES"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="@color/black"
                    android:textStyle="bold" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MUROS *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B" />

                <MultiAutoCompleteTextView
                    android:id="@+id/multiCompleteMuros"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Seleccione estructuras"
                    android:inputType="text" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="REVESTIMIENTO *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>
                <MultiAutoCompleteTextView
                    android:id="@+id/multiCompleteRevestimiento"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Seleccione estructuras"
                    android:inputType="text" />

                <TextView
                    android:id="@+id/tv_otros_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="OTRO"
                    android:layout_marginTop="11dp"
                    android:textColor="#00796B"
                    android:textSize="@dimen/text_size_small" />

                <EditText
                    android:id="@+id/edt_otros_albañeria"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:drawable/edit_text"
                    android:paddingStart="15dp"
                    android:paddingEnd="15dp"
                    android:hint="ESPECIFICAR"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:textSize="@dimen/text_size_small" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp"
                android:layout_marginTop="11dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PISOS"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:layout_marginBottom="5dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pisos *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"
                    android:layout_marginBottom="5dp"/>

                <MultiAutoCompleteTextView
                    android:id="@+id/multiCompletePisos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Seleccione estructuras"
                    android:inputType="text" />

                <TextView
