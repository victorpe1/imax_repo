                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="11dp"
                    android:text="SISTEMAS Puertas *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"/>
