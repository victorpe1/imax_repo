                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="16dp"
                android:layout_marginTop="11dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ESTRUCTURA"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:layout_marginBottom="5dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Estructura *"
                    android:textSize="@dimen/text_size_small"
                    android:textColor="#00796B"
                    android:layout_marginBottom="5dp"/>

                <MultiAutoCompleteTextView
                    android:id="@+id/multiCompleteEstructura"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Seleccione estructuras"
                    android:inputType="text" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OTRO"
                    android:layout_marginTop="11dp"
                    android:textColor="#00796B"
                    android:textSize="@dimen/text_size_small"/>

                <EditText
                    android:id="@+id/edt_otro_estructura"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:drawable/edit_text"                    android:padding="10dp"
                    android:hint="Especificar"
                    android:gravity="top"
                    android:textSize="@dimen/text_size_small" />
            </LinearLayout>
            <LinearLayout
